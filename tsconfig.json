{
  "extends": "@tsconfig/node18/tsconfig.json",
  "include": ["**/*.tsx"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "ai-jsx",
    "moduleResolution": "node16",
    "module": "node16",
    "resolveJsonModule": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
}